{
  "name": "eml-parse-js",
  "version": "1.2.0-beta.1",
  "description": "format EML file in browser env",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "browser": "./dist/index.es.js",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.cjs.js",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.es.js"
    },
    "./parser": {
      "types": "./dist/parser.d.ts",
      "node": "./dist/parser.cjs.js",
      "import": "./dist/parser.es.js",
      "require": "./dist/parser.cjs.js",
      "default": "./dist/parser.es.js"
    },
    "./builder": {
      "types": "./dist/builder.d.ts",
      "node": "./dist/builder.cjs.js",
      "import": "./dist/builder.es.js",
      "require": "./dist/builder.cjs.js",
      "default": "./dist/builder.es.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:publish": "npm run clean:dist && npm run build",
    "clean:dist": "rimraf dist",
    "eslint:init": "eslint --init",
    "lint": "eslint --cache --fix src",
    "prettier": "prettier --parser typescript --write src/*ts && npm run lint",
    "prepublishOnly": "npm run prettier && npm run build:publish && npm run test",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karladler/emlmo"
  },
  "keywords": [
    "eml",
    "eml-parse",
    "eml-parser",
    "eml-format-js",
    "eml-parse-js",
    "typescript"
  ],
  "author": "bean",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karladler/emlmo/issues"
  },
  "homepage": "https://github.com/karladler/emlmo#readme",
  "contributors": [
    {
      "name": "Charlie Harding",
      "url": "https://github.com/c-harding"
    },
    {
      "name": "PÃ¡draig Weeks",
      "url": "https://github.com/pmweeks98"
    },
    {
      "name": "Thomas Oeser",
      "url": "https://github.com/thomasoeser"
    },
    {
      "name": "Robert Scheinpflug",
      "url": "https://github.com/neversun"
    },
    {
      "name": "Bean Q",
      "url": "https://github.com/MQpeng"
    },
    {
      "name": "Karl Adler",
      "url": "https://github.com/karladler"
    }
  ],
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^2.1.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "packageManager": "pnpm@9.15.1"
}
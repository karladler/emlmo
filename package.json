{
  "name": "emlmo",
  "version": "0.0.1",
  "description": "EML (RFC 5322) parser",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "browser": "./dist/index.es.js",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.cjs.js",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.es.js"
    },
    "./parser": {
      "types": "./dist/parser.d.ts",
      "node": "./dist/parser.cjs.js",
      "import": "./dist/parser.es.js",
      "require": "./dist/parser.cjs.js",
      "default": "./dist/parser.es.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:publish": "npm run build",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "types:check": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run build:publish && npm run test",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karladler/emlmo"
  },
  "keywords": [
    "eml",
    "eml-parse",
    "eml-parser",
    "eml-format-js",
    "eml-parse-js",
    "email-parser",
    "typescript"
  ],
  "author": "Karl Adler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karladler/emlmo/issues"
  },
  "homepage": "https://github.com/karladler/emlmo#readme",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "packageManager": "pnpm@10.30.1"
}